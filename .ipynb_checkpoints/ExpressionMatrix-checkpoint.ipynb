    "xyset = range(0,3)\n",
    "\n",
    "\n",
    "expr = (4+x)*(2-y)\n",
    "\n",
    "for i in range(0,3):\n",
    "    for j in range(0,3):\n",
    "        sym.latex(expr.subs(x,i).subs(y,j).evalf())\n",
    "\n"
